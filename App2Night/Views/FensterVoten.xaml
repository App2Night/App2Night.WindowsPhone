<Page
    x:Class="App2Night.Views.BlankPage1"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:App2Night.Views"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">
    <Page.BottomAppBar>
        <CommandBar>
            <CommandBar.Content>
                <Grid/>
            </CommandBar.Content>
            <AppBarButton x:Name="AppBarButtonGefallen_" Icon="Like" Width="150" Label="Gefallen"/>
            <AppBarButton x:Name="AppBarButtonNichtGefallen" Icon="Dislike" Width="150" Label="Nicht gefallen"/>
        </CommandBar>
    </Page.BottomAppBar>

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Margin="10,166,0,0" TextWrapping="Wrap" Text="Hat Dir die Party gefallen?             Bitte voten" VerticalAlignment="Top" Width="340" Height="106" FontSize="20"/>

    </Grid>
</Page>
